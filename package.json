{
  "name": "mu2",
  "description": "Template for typescript projects",
  "version": "1.0.0",
  "author": "Rajat Sharma",
  "bugs": {
    "url": "https://github.com/rajatsharma/mu2/issues"
  },
  "devDependencies": {
    "ava": "^3.1.0",
    "esbuild": "^0.14.11",
    "eslint-config-rsh": "https://github.com/rajatsharma/eslint-config-rsh"
  },
  "eslintConfig": {
    "extends": "rsh"
  },
  "engines": {
    "node": ">=10.18.0"
  },
  "homepage": "https://github.com/rajatsharma/mu2#readme",
  "keywords": [
    "editorconfig",
    "eslint",
    "repos",
    "ts"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajatsharma/mu2.git"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outdir=dist --format=cjs",
    "dev": "esbuild src/index.ts --watch --outdir=dist",
    "test": "ava"
  }
}
